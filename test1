#!/bin/bash

current_date=$(date +"%Y-%m-%d")
date=$(date -d "$current_date - 1 day" +"%Y-%m-%d")

# Extract the "mm" and "dd" part of yesterday's date
mm_dd=$(date -d "$date" +"%m-%d")

# Set the output directory path
output_dir="/data/orsa-intermediate"

# Create the output directory if it doesn't exist
mkdir -p "$output_dir"

# Move files matching the pattern "*mm-dd.csv" to the output directory
mv /data/pvellanki/orsa-package-2/*"$mm_dd.csv" "$output_dir/"
