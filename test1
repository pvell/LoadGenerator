from deephaven import init, TableOperations

# Connect to Deephaven instance
init(host="your_remote_host", port=your_remote_port)


# Get a list of all tables
all_tables = TableOperations.list_tables()

# Print the list of tables
for table_name in all_tables:
    print(f"Table: {table_name}")

# Close the connection to Deephaven
TableOperations.shutdown()


