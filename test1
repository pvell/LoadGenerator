from deephaven import Session

# Set Deephaven server address and port
deephaven_server = "localhost"
deephaven_port = 10000

# Connect to Deephaven
session = Session(deephaven_server, deephaven_port)

# Assuming you have a table named "Orders," you can query it
orders_table = session.query("Orders")
print(orders_table.head())

# Don't forget to close the session when you're done
session.close()
