def onLogout(self, sessionID):
    print("Logout initiated -", sessionID.toString())
    
    # Log the initiation of Logout
    log_file_path = self.get_log_file()
    with open(log_file_path, "a") as log_file:
        log_file.write("Logout initiated - {}\n".format(sessionID.toString()))

    # Create a Logout message
    logout_message = fix.Message()
    header = logout_message.getHeader()
    header.setField(fix.MsgType(fix.MsgType_Logout))
    
    # You can add more fields to the Logout message as needed
    
    # Send the Logout message
    fix.Session.sendToTarget(logout_message, sessionID)
