import deephaven.TableTools as tt

# Set Deephaven server address and port
deephaven_server = "localhost"
deephaven_port = 10000

# Connect to Deephaven
deephaven_connection = tt.connect(deephaven_server, deephaven_port)

# Assuming you have a table named "Orders," you can query it
orders_table = tt.readTable(deephaven_connection, "Orders")
print(tt.head(orders_table))

# Don't forget to close the connection when you're done
tt.close(deephaven_connection)
